<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">

  <Styles>
    <Style ss:ID="s8">
     <Alignment ss:Horizontal="Center" ss:Vertical="Center"/>
     <Borders>
      <Border ss:Position="Bottom" ss:LineStyle="Continuous" ss:Weight="1"/>
      <Border ss:Position="Left" ss:LineStyle="Continuous" ss:Weight="1"/>
      <Border ss:Position="Right" ss:LineStyle="Continuous" ss:Weight="1"/>
      <Border ss:Position="Top" ss:LineStyle="Continuous" ss:Weight="1"/>
     </Borders>
     <Font x:Family="Swiss" ss:Size="12" ss:Bold="1"/>
    </Style>
  </Styles>

  <Worksheet ss:Name="ReporteRegistroUnico">
    <Table>

      <Row ss:Height="60">
        <Cell ss:MergeAcross="5" ss:StyleID="s8">
          <Data ss:Type="String">
            <%= "LA PREVISORA S.A. COMPANIA DE SEGUROS" %>
          </Data>
        </Cell>
      </Row>

      <Row>
        <Cell ss:MergeAcross="5" ss:StyleID="s8">
          <Data ss:Type="String">
            <%= "VICEPRESIDENCIA JURIDICA" %>
          </Data>
        </Cell>
      </Row>

      <Row>
        <Cell ss:MergeAcross="5" ss:StyleID="s8">
          <Data ss:Type="String">
            <%= "REPORTE DE PROCESOS REGISTRADOS" %>
          </Data>
        </Cell>
      </Row>

      <Row>
        <Cell ss:MergeAcross="5" ss:StyleID="s8">
          <Data ss:Type="String">
            <%= "FECHA DE GENERACION: #{Date.today}" %>
          </Data>
        </Cell>
      </Row>

      <Row>
        <Cell ss:MergeAcross="5" ss:StyleID="s8">
          <Data ss:Type="String">
            <%= "DESDE: #{@date_from} HASTA: #{@date_till}" %>
          </Data>
        </Cell>
      </Row>

      <Row></Row>

      <Row>
        <% @report.column_names_all.each do |c| %>
          <Cell><Data ss:Type="String"><%= c %></Data></Cell>
        <% end %>
      </Row>


      <% @report.each do |process| %>
        <Row>
          <% process.get_content_all.each do |fields| %>
            <Cell><Data ss:Type="AnyType"><%= fields %></Data></Cell>
          <% end %>
        </Row>
      <% end %>

    </Table>
  </Worksheet>
</Workbook>
