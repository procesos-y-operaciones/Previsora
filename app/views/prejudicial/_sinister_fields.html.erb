<div class='nested-fields'>
  <hr>
  <%= f.label :reserve_cents, "Número del Siniestro" %>
  <div class="input-group">
    <%= f.text_field :reserve_cents, maxlength:"20", pattern:"[0-9\,\' ]*", title: t(:pattern_n), onclick:"formatting_number(this)", onkeyup:"formatting_number(this)", onblur:"formatting_empty_number(this)", class:"form-control" %>
    <span data-toggle="tooltip" data-html="true" title="<%= t('processes.prejudicial.reserved_fees_cents_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
  </div>

  <%= f.label :reserve_cents, "Reserva de la indemnización" %>
  <div class="input-group">
    <%= f.text_field :reserve_cents, maxlength:"20", pattern:"[0-9\,\' ]*", title: t(:pattern_n), onclick:"formatting_number(this)", onkeyup:"formatting_number(this)", onblur:"formatting_empty_number(this)", class:"form-control" %>
    <span data-toggle="tooltip" data-html="true" title="<%= t('processes.prejudicial.reserved_fees_cents_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
  </div>

  <%= f.label :reserve_cents, "Reserva honorarios" %>
  <div class="input-group">
    <%= f.text_field :reserved_fees_cents, maxlength:"20", pattern:"[0-9\,\' ]*", title: t(:pattern_n), onclick:"formatting_number(this)", onkeyup:"formatting_number(this)", onblur:"formatting_empty_number(this)", class:"form-control" %>
    <span data-toggle="tooltip" data-html="true" title="<%= t('processes.prejudicial.reserved_fees_cents_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
  </div>

  <div class="input-group">
    <%= link_to_remove_association f, class: "minibtn btn-default m-1" do %>
      <span class="fa">&#x2212;</span>
    <% end %>
  </div>
  <hr>
</div>
