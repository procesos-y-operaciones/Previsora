        </select>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.fiscal.city_case_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <hr>

      <%= form.label :litigation_source, "Fuente de litigio" %>
