      <%= form.label :fail_value_cents, "Valor del fallo de la decisión / sanción" %>
      <div class="input-group">
        <%= form.text_field :fail_value_cents, id:"fail_value_cents", maxlength:"20", pattern:"[0-9\,\' ]*", title: t(:pattern_n), onclick:"formatting_number(this)", onkeyup:"formatting_number(this)", onblur:"formatting_empty_number(this)", class:"form-control" %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.fiscal.fail_value_cents_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :fail_previ_cents, "Valor del fallo / sanción a cargo de Previsora" %>
      <div class="input-group">
        <%= form.text_field :fail_previ_cents, id:"fail_previ_cents", maxlength:"20", pattern:"[0-9\,\' ]*", title: t(:pattern_n), onclick:"formatting_number(this)", onkeyup:"formatting_number(this)", onblur:"formatting_empty_number(this)", class:"form-control" %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.fiscal.fail_previ_cents_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :payed_value_cents, "Valor pagado por Previsora" %>
      <div class="input-group">
        <%= form.text_field :payed_value_cents, id:"payed_value_cents", maxlength:"20", pattern:"[0-9\,\' ]*", title: t(:pattern_n), onclick:"formatting_number(this)", onkeyup:"formatting_number(this)", onblur:"formatting_empty_number(this)", class:"form-control" %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.fiscal.payed_value_cents_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :payment_date, "Fecha de consignación / pago" %>
      <div class="input-group">
        <%= form.date_field :payment_date, id:"payment_date", class:"form-control" %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.fiscal.payment_date_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :has_coactive_radicate, "¿Tiene número de radicado de coactivo?" %>
      <div class="input-group">
        <%= form.select :has_coactive_radicate, options_for_select([["NO", false], ["SI", true]]), {}, {id: "coactive", class:"form-control"} %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.judicial.coactive_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :coactive_radicate, "Número de radicación del coactivo" %>
      <div class="input-group">
        <%= form.text_field :coactive_radicate, id:"coactive_radicate", pattern:"[A-Z0-9\- ]*", title: t(:pattern), onclick:"input_nopresenta_rule(this)", onblur:"input_empty_nopresenta_rule(this)", class:"form-control" %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.fiscal.coactive_radicate_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :coactive_value_cents, "Valor del coactivo" %>
      <div class="input-group">
        <%= form.text_field :coactive_value_cents, id:"coactive_value_cents", maxlength:"20", pattern:"[0-9\,\' ]*", title: t(:pattern_n), onclick:"formatting_number(this)", onkeyup:"formatting_number(this)", onblur:"formatting_empty_number(this)", class:"form-control" %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.fiscal.coactive_value_cents_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
