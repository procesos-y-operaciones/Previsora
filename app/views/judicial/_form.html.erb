        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.judicial.litigation_source_id_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :policy_cents, "Póliza" %>
