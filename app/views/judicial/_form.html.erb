      </div>

      <%= form.label :branch_commercial, "Ramo comercial" %>
      <div class="input-group">
        <%= form.collection_select(:branch_commercial, BranchCommercial.all, :num, :large_name, {}, {class:"form-control", :id => "branch_commercial" }) %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.judicial.branch_commercial_id_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :sinister, "Número del siniestro (completo)" %>
      <div class="input-group">
        <%= form.text_field :sinister, id:"sinister", readonly: true, class:"form-control" %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.judicial.sinister_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :more_policies, "¿Tiene más pólizas?" %>
      <div class="input-group">
        <select id="more_policies" class="form-control">
          <option value="Si">SI</option>
          <option value="No">NO</option>
        </select>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.judicial.more_policies_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :policies, "Número de pólizas" %>
      <div class="input-group">
        <%= form.text_area :policies, id:"policies", maxlength:"255", pattern:"[0-9\- ]*", title: t(:pattern), onclick:"input_pendiente_rule(this)", onblur:"input_empty_pendiente_rule(this)", class:"form-control" %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.judicial.policies_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :sinisters, "Número de siniestros" %>
      <div class="input-group">
        <%= form.text_area :sinisters, id:"sinisters", maxlength:"255", pattern:"[0-9\- ]*", title: t(:pattern), onclick:"input_pendiente_rule(this)", onblur:"input_empty_pendiente_rule(this)", class:"form-control" %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.judicial.sinisters_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <hr>

      <%= form.label :money_type, "Tipo de moneda" %>
      <div class="input-group">
        <%= form.collection_select(:money_type, MoneyType.all, :name, :name, {}, {class:"form-control", id:"moneyType"}) %>
