      </div>

      <%= form.label :coensurance_type, "Tipo de coaseguro" %>
      <div class="input-group">
        <%= form.collection_select(:coensurance_type, CoensuranceType.all, :name, :name, {include_blank: 'SELECCIONE'}, {id: "coensurance_type", class:"form-control"}) %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.judicial.coensurance_type_id_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <%= form.label :coensurance_value_cents, "Valor del coaseguro" %>
      <div class="input-group">
        <%= form.text_field :coensurance_value_cents, id:"coensurance_value", onclick:"formatting_number(this)", onkeyup:"formatting_number(this)", onblur:"formatting_empty_number(this)", class:"form-control"%>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.judicial.coensurance_value_cents_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
      </div>

      <hr>

      <%= form.label :litigation_source, "Fuente del litigio" %>
