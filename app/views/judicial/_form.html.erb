        <%= form.number_field :number, id:"number", class:"form-control" %>
        <span data-toggle="tooltip" data-html="true" title="<%= t('processes.judicial.number_d') %>" class="input-group-addon fa fa-question m-1"  style="font-size:20px"></span>
